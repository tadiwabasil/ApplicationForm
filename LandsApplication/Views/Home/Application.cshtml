@model LandsApplication.Models.Application

@{
    ViewData["Title"] = "Application";
}

<h1>Application</h1>

<h4>Application</h4>
<hr />


<div class="row">
    <div class="col-md-8 offset-2">
        <form asp-action="Index">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Forenames" class="control-label"></label>
                <input asp-for="Forenames" class="form-control" />
                <span asp-validation-for="Forenames" class="text-danger"></span>
            </div>

            <div class="form-group">
                <label asp-for="Surname" class="control-label"></label>
                <input asp-for="Surname" class="form-control" />
                <span asp-validation-for="Surname" class="text-danger"></span>
            </div>

            <div class="form-group">
                <label asp-for="MaritalStatus" class="control-label"></label>
                <select asp-for="MaritalStatus" class="form-control" asp-items="Html.GetEnumSelectList<MaritalStatus>()"> </select>
                <span asp-validation-for="MaritalStatus" class="text-danger"></span>
            </div>

            <div class="form-group">

                <label asp-for="SpouseName" class="control-label"></label>
                <input asp-for="SpouseName" class="form-control" />
                <span asp-validation-for="SpouseName" class="text-danger"></span>
            </div>

            <div class="form-control">
                <div class="form-group">
                    <label asp-for="DateOfBirth.Applicant" class="control-label"></label>
                    <input asp-for="DateOfBirth.Applicant" class="form-control" />
                    <span asp-validation-for="DateOfBirth.Applicant" class="text-danger"></span>
                </div>

                <div class="form-group">
                    <label asp-for="DateOfBirth.Spouse" class="control-label"></label>
