                </div>

                <div class="form-group">
                    <label asp-for="DateOfBirth.RegistrationDate" class="control-label"></label>
                    <input asp-for="DateOfBirth.RegistrationDate" class="form-control" />
                    <span asp-validation-for="DateOfBirth.RegistrationDate" class="text-danger"></span>
                </div>
            </div>

            <div class="form-control">
                <div class="form-group">

                    <label asp-for="RegistrationParticulars.Applicant" class="control-label"></label>
                    <input asp-for="RegistrationParticulars.Applicant" class="form-control" />
                    <span asp-validation-for="RegistrationParticulars.Applicant" class="text-danger"></span>

                </div>

                <div class="form-group">
                    <label asp-for="RegistrationParticulars.Spouse" class="control-label"></label>
                    <input asp-for="RegistrationParticulars.Spouse" class="form-control" />
                    <span asp-validation-for="RegistrationParticulars.Spouse" class="text-danger"></span>
