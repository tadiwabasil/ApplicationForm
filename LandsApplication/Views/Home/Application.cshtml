                <label class="form-check-label">
                    <input class="form-check-input" asp-for="Insolvantion" /> @Html.DisplayNameFor(model => model.Insolvantion)
                </label>
            </div>


            <div class="form-group">
                <label asp-for="FinanceSource" class="control-label"></label>
                <input asp-for="FinanceSource" class="form-control" />
                <span asp-validation-for="FinanceSource" class="text-danger"></span>
            </div>


